colnames(fcgeneexpression)=metadata[1:819,3]
fc_DCK <- fcgeneexpression["DCK",]
plot(fc_DCK, col = metadata[1:819,3])
fc_DCK_gemci <- fc_DCK[365:420]
drugsensitivity_gemci <- drugsensitivity["gemcitibine",]
a <- rbind(drugsensitivity_gemci, fc_DCK_gemci)
a_no_nas <- a[,-c(29,18)]
fc_of_DCK_gemci <- as.vector(a_no_nas[2,])
sensitivity <- as.vector(a_no_nas[1,])
plot(fc_of_DCK_gemci,sensitivity)
abline(lm(fc_of_DCK_gemci ~ sensitivity))
